PS5_PAYLOAD_SDK="${BASH_SOURCE[0]}"
PS5_PAYLOAD_SDK="$(dirname "${PS5_PAYLOAD_SDK}")"
PS5_PAYLOAD_SDK="${PS5_PAYLOAD_SDK}/.."

CANDIDATES=("llvm-objcopy-18"
	    "llvm-objcopy-17"
            "llvm-objcopy-16"
            "llvm-objcopy-15")

for OBJCOPY in "${CANDIDATES[@]}"; do
    if [ -x "$(command -v $OBJCOPY)" ]; then
	exec $OBJCOPY $*
    fi
done

exit 1
